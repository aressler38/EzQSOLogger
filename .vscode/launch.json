{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start App",
      "type": "coreclr",
      "request": "attach",
      "justMyCode": true,
      "preLaunchTask": "Combined tasks",
      "processId": "${command:azureFunctions.pickProcess}"
    },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "https://localhost:4280",
      "webRoot": "${workspaceRoot}/EzQSOLogger/app/src",
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}